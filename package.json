{
  "name": "Apollo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test-chrome": "node-debug --preload false --nodejs --harmony jest --runInBand"
  },
  "jest": {
    "preset": "jest-react-native",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(es6|js|json)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "ios",
        "android"
      ],
      "providesModuleNodeModules": [
        "react",
        "react-native"
      ]
    },
    "collectCoverage": false,
    "verbose": true
  },
  "dependencies": {
    "apollo-client": "0.4.21",
    "graphql-tag": "0.1.14",
    "immutable": "3.8.1",
    "react": "15.3.2",
    "react-apollo": "0.5.11",
    "react-native": "0.35.0",
    "react-native-router-flux": "3.36.0",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "16.0.0",
    "babel-preset-react-native": "1.9.0",
    "babel-root-import": "4.1.1",
    "docker": "0.2.14",
    "eslint": "3.4.0",
    "eslint-config-standard": "6.0.0",
    "eslint-config-standard-react": "4.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.2.0",
    "eslint-plugin-standard": "2.0.0",
    "istanbul": "0.4.5",
    "jest": "16.0.1",
    "jest-cli": "16.0.1",
    "jest-react-native": "16.0.0",
    "nock": "8.0.0",
    "react-addons-test-utils": "15.3.1",
    "react-test-renderer": "15.3.2",
    "redux-mock-store": "1.0.3"
  }
}
